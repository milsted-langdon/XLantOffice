@model XLantCore.Models.MLFSIncome

<hr />
<div class="row">
    <div class="col-md-4">
        <form id="alterForm" asp-action="AlterAdvisor">
            @Html.HiddenFor(model => model.Id)
            <div class="form-group">
                <label asp-for="AdvisorId" class="control-label">Advisor</label>
                <select asp-for="AdvisorId" class="form-control" asp-items="ViewBag.AdvisorId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
