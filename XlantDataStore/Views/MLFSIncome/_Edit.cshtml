@model XLantCore.Models.MLFSIncome

<h4>MLFSIncome</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="ReportingPeriodId" />
            <div class="form-group">
                <label asp-for="IOReference" class="control-label"></label>
                <input asp-for="IOReference" class="form-control" />
                <span asp-validation-for="IOReference" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RelevantDate" class="control-label"></label>
                <input asp-for="RelevantDate" class="form-control" />
                <span asp-validation-for="RelevantDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Organisation" class="control-label"></label>
                <input asp-for="Organisation" class="form-control" />
                <span asp-validation-for="Organisation" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdvisorId" class="control-label"></label>
                <select asp-for="AdvisorId" class="form-control" asp-items="ViewBag.AdvisorId"></select>
                <span asp-validation-for="AdvisorId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProviderName" class="control-label"></label>
                <input asp-for="ProviderName" class="form-control" />
                <span asp-validation-for="ProviderName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientName" class="control-label"></label>
                <input asp-for="ClientName" class="form-control" />
                <span asp-validation-for="ClientName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientId" class="control-label"></label>
                <input asp-for="ClientId" class="form-control" />
                <span asp-validation-for="ClientId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="JointClientName" class="control-label"></label>
                <input asp-for="JointClientName" class="form-control" />
                <span asp-validation-for="JointClientName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="JointClientId" class="control-label"></label>
                <input asp-for="JointClientId" class="form-control" />
                <span asp-validation-for="JointClientId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Campaign" class="control-label"></label>
                <input asp-for="Campaign" class="form-control" />
                <span asp-validation-for="Campaign" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CampaignSource" class="control-label"></label>
                <input asp-for="CampaignSource" class="form-control" />
                <span asp-validation-for="CampaignSource" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Amount" class="control-label"></label>
                <input asp-for="Amount" class="form-control" />
                <span asp-validation-for="Amount" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VAT" class="control-label"></label>
                <input asp-for="VAT" class="form-control" />
                <span asp-validation-for="VAT" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FeeStatus" class="control-label"></label>
                <input asp-for="FeeStatus" class="form-control" />
                <span asp-validation-for="FeeStatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PlanType" class="control-label"></label>
                <input asp-for="PlanType" class="form-control" />
                <span asp-validation-for="PlanType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PlanNumber" class="control-label"></label>
                <input asp-for="PlanNumber" class="form-control" />
                <span asp-validation-for="PlanNumber" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsTopUp" /> @Html.DisplayNameFor(model => model.IsTopUp)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="IncomeType" class="control-label"></label>
                <input asp-for="IncomeType" class="form-control" />
                <span asp-validation-for="IncomeType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientOnBoardDate" class="control-label"></label>
                <input asp-for="ClientOnBoardDate" class="form-control" />
                <span asp-validation-for="ClientOnBoardDate" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsClawBack" /> @Html.DisplayNameFor(model => model.IsClawBack)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsAdjustment" /> @Html.DisplayNameFor(model => model.IsAdjustment)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IgnoreFromCommission" /> @Html.DisplayNameFor(model => model.IgnoreFromCommission)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
